{
  "name": "rxweb",
  "version": "0.0.0-development",
  "scripts": {
    "commit": "git-cz",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release",
    "build:validators": "ng-packagr -p ./packages/reactive-form-validators/package.json"
  },
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@rxweb/dom": "0.0.3",
    "@rxweb/localization": "0.0.2-beta2",
    "@rxweb/reactive-form-validators": "^1.9.2",
    "@rxweb/translate": "^1.0.14",
    "codecov": "^3.1.0",
    "commitizen": "^4.3.0",
    "core-js": "2.6.5",
    "cz-conventional-changelog": "^2.1.0",
    "rxjs": "~7.5.0",
    "time-ago-pipe": "^1.3.2",
    "tslib": "^2.3.0",
    "validate-commit-msg": "^1.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.1",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "~12.0.1",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^0.0.28",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.1.0",
    "protractor": "^3.3.0",
    "semantic-release": "^21.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.7.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test",
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxweb/rxweb.git"
  }
}
